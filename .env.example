# Copy to .env and fill in values

# Timezone
TZ=America/Los_Angeles

# Domain for traefik routing
DOMAIN=home.local

# GitHub/Gitea token for cloning repos
GIT_ACCESS_TOKEN=ghp_xxxxxxxxxxxx

# Your gitops repo URL
GITOPS_REPO_URL=https://github.com/youruser/home-ops.git

# HTTPS / Let's Encrypt
ACME_EMAIL=admin@example.com
CF_DNS_API_TOKEN=  # cloudflare API token (for DNS challenge)

# Replicas (for zero-downtime rolling updates)
REPLICAS=2
